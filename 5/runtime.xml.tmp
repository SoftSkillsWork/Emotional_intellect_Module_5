<script>
  Vue.component('lmm-test-container', {
    template: '#lmm-test-container-template',
    props: {
      data: {
        type: Array
      },
      random: {
        type: Boolean
      },
      closeTestCallback: {
        type: Function,
        default: function () {}
      },
      questionThreshold: {
        type: Number
      },
      questionNumberShow: {
        type: Boolean
      }
    },
    data: function () {
      return {
        curQuestionNumber: 0,
        isNextButtonShow: false,
        isApplyQuestionButtonShow: false,
        isCorrectAnswer: false,
        isInCorrectAnswer: false,
        now: Date.now(),
        questions: [],
        showFeedback: false,
        feedbackText: ''
      };
    },
    mounted: function () {
      if (this.random) {
        this.shuffleQuestions();
      }
      var vm = this;
      EventBus.$on('reset-assessment', function () {
        vm.resetAssessment();
      });
    },
    methods: {
      getData: function () {
        this.data = this.data.slice();
      },
      shuffleQuestions: function () {
        this.getData();
        this.data.sort(function (el) {
          return .5 - Math.random();
        });
        this.data = this.data.slice(0, this.questionThreshold);
        this.data.forEach(function (el) {
          el.answers.sort(function () {
            return .5 - Math.random();
          });
        });
      },
      resetAssessment: function () {
        CLV.oSlide["test_score"] = 0;
        this.now = Date.now();
        this.isCorrectAnswer = false;
        this.isInCorrectAnswer = false;
        this.curQuestionNumber = 0;
        this.isApplyQuestionButtonShow = false;
        this.isNextButtonShow = false;
        this.data.forEach(function (question) {
          question.answers.forEach(function (answer) {
            answer.disabled = false;
            answer.is_selected = false;
          });
        });
        this.shuffleQuestions();
      },
      applyQuestion: function () {
        this.now = Date.now();
        var is_correct = this.checkAnswer();
        this.isCorrectAnswer = is_correct;
        this.isInCorrectAnswer = !is_correct;
        this.isCorrectAnswer && (CLV.oSlide["test_score"] = ++CLV.oSlide["test_score"] || 1);
        this.showFeedback = true;
        this.isApplyQuestionButtonShow = false;
        this.isNextButtonShow = true;
        if (this.curQuestionNumber === this.data.length - 1) {
          this.nextButtonText = 'Продолжить';
        } else {
          this.nextButtonText = 'Следующий вопрос';
        }
        var a = this.data[this.curQuestionNumber].answers;
        for (var i = 0; i < a.length; i++) {
          a[i].disabled = true;
        }
      },
      changeRadioAnswer: function ($event, data) {
        var a = this.data[this.curQuestionNumber].answers;
        for (var i = 0; i < a.length; i++) {
          a[i].is_selected = false;
        }
        this.feedbackText = data.feedback;
        this.isApplyQuestionButtonShow = data.is_selected = $event.target.checked;
      },
      changeCheckboxAnswer: function ($event, data) {
        data.is_selected = $event.target.checked;
        var a = this.data[this.curQuestionNumber].answers;
        this.isApplyQuestionButtonShow = false;
        for (var i = 0; i < a.length; i++) {
          if (a[i].is_selected) {
            return this.isApplyQuestionButtonShow = true;
          }
        }
      },
      closeTest: function () {
        this.closeTestCallback();
        // if (CLF[CLZ.sCurrentFrameId].bIsLast) {
        //     CL.Mask({
        //         on: true
        //     });
        //     setTimeout(function() {
        //         $('#next_sl td').click()
        //     }, 100);
        // } else {
        //     $('#next_fr td').click();
        // }
      },
      checkRadioType: function () {
        var a = this.data[this.curQuestionNumber].answers;
        for (var i = 0; i < a.length; i++) {
          if ((a[i].is_correct && !a[i].is_selected) || (!a[i].is_correct && a[i].is_selected)) {
            return false;
          }
        }
        return true;
      },
      checkCheckboxType: function () {
        var a = this.data[this.curQuestionNumber].answers;
        for (var i = 0; i < a.length; i++) {
          if ((a[i].is_correct && !a[i].is_selected) || (!a[i].is_correct && a[i].is_selected)) {
            return false;
          }
        }
        return true;
      },
      checkAnswer: function () {
        if (this.data[this.curQuestionNumber].question_type === 'radio') {
          return this.checkRadioType();
        } else if (this.data[this.curQuestionNumber].question_type === 'checkbox') {
          return this.checkCheckboxType();
        }
        throw 'Неизвестный тип теста';
      },
      nextQuestion: function ($event) {
        this.showFeedback = true;
        // this.feedbackText = '';
        if (this.curQuestionNumber === this.data.length - 1) {
          this.isNextButtonShow = false;
          return this.closeTest();
        }
        this.curQuestionNumber++;
        this.isNextButtonShow = false;
        this.isCorrectAnswer = false;
        this.isInCorrectAnswer = false;
        this.showFeedback = false;
      }
    }
  });
</script>
