                CL.Event.Subscribe({ aList: [ { sId: argobj.pid, sEvtName: "EVENT_SLIDE_OPENED" },{ sId: argobj.pid, sEvtName: "EVENT_FRAME_OPENED" }  ] });
                return true;
            }                                                                                                                                                                                                                                                                                                                                                                                                                                ]]>
