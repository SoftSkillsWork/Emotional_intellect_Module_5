<objective id="test_try1" name="Первая попытка" cs="n" ss="u" scoremax="0" scoremin="0"/>
<objective id="test_try2" name="Вторая попытка" cs="n" ss="u" scoremax="0" scoremin="0"/>
<objective id="test_try3" name="Третья попытка" cs="n" ss="u" scoremax="0" scoremin="0"/>
<objective id="FirstName" name="Обращение к пользователю" cs="n" ss="u" scoremax="0" scoremin="0"/>
</objectives>
<rules>
<rule objective="total" cs="c" ss="p">
<condition type="visited" visits="2v,2x"/>
<condition type="score" op="and" objective="total" comp="ge" score="80"/>
</rule>
<rule objective="total" cs="c" ss="f">
<condition type="visited" visits="2v,2x"/>
<condition type="score" op="and" objective="total" comp="lt" score="80"/>
</rule>
</rules>
