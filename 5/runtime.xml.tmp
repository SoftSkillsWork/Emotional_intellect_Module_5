<script>
  Vue.component('lmm-draggable', {
    template: '#lmm-draggable-template',
    props: {
      data: {
        type: Array,
        default: function () {
          return [];
        }
      },
      titleHeader: {
        type: String,
        default: function () {
          return '';
        }
      },
      header: {
        type: String,
        default: function () {
          return '';
        }
      },
      hint: {
        type: String,
        default: function () {
          return '';
        }
      },
      leftButtonText: {
        type: String,
        default: function () {
          return '';
        }
      },
      rightButtonText: {
        type: String,
        default: function () {
          return '';
        }
      },
      answerButtonText: {
        type: String,
        default: function () {
          return '';
        }
      },
      answerClickCallback: {
        type: Function,
        default: function () {}
      }
    },
    mounted: function () {
      this.sortedQuestions();
      this.initDraggable();
      this.initDroppable();
      CLV.oSlide['pr_score'] = 0;
      var vm = this;
      EventBus.$on('reset-draggable', function () {
        vm.resetClickCallback();
      });
    },
    data: function () {
      return {
        curQuestionIndex: 0,
        isButtonBlockShow: false,
        isButtonResetShow: false,
        isDraggableShow: true
      }
    },
    methods: {
      sortedQuestions: function () {
        this.data.sort(function () {
          return .5 - Math.random();
        });
      },
      resetClickCallback: function () {
        CLV.oSlide['pr_score'] = 0;
        this.isDraggableShow = true;
        this.isButtonBlockShow = false;
        this.isButtonResetShow = false;
        this.curQuestionIndex = 0;
        this.sortedQuestions();
      },
      checkSide: function (side) {
        this.isButtonResetShow = true;
        if (this.$refs.draggable.getAttribute('side') === side) {
          ++CLV.oSlide['pr_score'];
        }
        if (this.curQuestionIndex < this.data.length - 1) {
          ++this.curQuestionIndex;
        } else {
          this.isDraggableShow = false;
          this.isButtonBlockShow = true;
        }
      },
      initDroppable: function () {
        var vm = this;
        $(vm.$refs.rightDroppable).droppable({
          drop: function () {
            vm.checkSide('right');
          }
        });
        $(vm.$refs.leftDroppable).droppable({
          drop: function () {
            vm.checkSide('left');
          }
        });
      },
      initDraggable: function () {
        $(this.$refs.draggable).draggable({
          scroll: false,
          axis: "x",
          revert: true,
          revertDuration: 1
        });
      },
      answerClick: function () {
        this.answerClickCallback();
      }
    }
  })
</script>
