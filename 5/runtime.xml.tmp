<script>
  Vue.component('lmm-menu', {
    template: '#lmm-menu-template',
    props: {
      showContent: {
        type: Boolean
      },
      showMaterials: {
        type: Boolean
      },
      showGlossary: {
        type: Boolean
      },
      materials: {
        type: Array,
        default: function () {
          return [];
        }
      },
      menuItems: {
        type: Array,
        default: function () {
          return [{
            subItem: []
          }];
        }
      },
      glossary: {
        type: Array,
        default: function () {
          return [];
        }
      }
    },
    data: function () {
      return {
        tabIndex: 0,
        currentMenuItemIndex: 0,
        currentMenuItemSubIndex: 0,
        show: false,
        curTitleIndex: 0,
        obj: {},
        obj2: {},
        prevVisited: undefined
      }
    },
    mounted: function () {
      var EventBus = window.EventBus || new Vue();
      var vm = this;
      var obj2 = this.obj2;
      EventBus.$on('close-menu', function () {
        vm.closeMenu();
      });
      EventBus.$on('open-menu', function () {
        vm.openMenu();
        modulNames();
      });
      EventBus.$on('menu-enabled', function (blockIndex) {
        blockIndex = parseInt(blockIndex);
        if (isNaN(blockIndex)) {
          return;
        }
        for (var i = 0; i <= blockIndex; i++) {
          vm.menuItems[blockIndex].disabled = false;
        }
      });
      EventBus.$on('create-anchor-array', function () {
        vm.createAnchorArray();
        modulNames();
      });
      $('.lmm-menu__content').scroll(function (e) {
        if (vm.preventScrollIndexDetection) {
          return;
        }
        var scrollContainer = e.target;
        var containerScrollTop = scrollContainer.scrollTop;
        var titlesArray = $(".lmm-menu__glossary-title[data-title-id]");
        var dataTitleID;
        for (var i = 0; i < titlesArray.length; i++) {
          var elementOffset = titlesArray[i].offsetTop;
          if (elementOffset <= containerScrollTop) {
            var anchor = titlesArray[i] || titlesArray[0];
            dataTitleID = $(anchor).attr('data-title-id');
          }
        }
        if (dataTitleID === undefined) {
          dataTitleID = $(titlesArray[0]).attr('data-title-id');
        }
        $('.lmm-menu__glossary-link').each(function (index) {
          if ($(this).text().trim() === dataTitleID.trim()) {
            vm.curTitleIndex = index;
          }
        });
      });
    },
    computed: {
      glossaryData: function () {
        var glossaryCopy = this.glossary.slice();
        var glossaryTitles = [];
        for (var i = 0; i < glossaryCopy.length; i++) {
          var firstCharacter = glossaryCopy[i].name.slice(0, 1);
          if (glossaryTitles.indexOf(firstCharacter) === -1) {
            glossaryTitles.push(firstCharacter);
          }
        }
        glossaryTitles.sort(function (a, b) {
          return a.localeCompare(b);
        });
        var glossaryData = [];
        var glossaryTempData = [];
        for (var i = 0; i < glossaryTitles.length; i++) {
          glossaryTempData = [];
          for (var j = 0; j < glossaryCopy.length; j++) {
            if (glossaryCopy[j].name.indexOf(glossaryTitles[i]) === 0) {
              glossaryTempData.push(glossaryCopy[j]);
            }
          }
          glossaryData.push({
            title: glossaryTitles[i],
            data: glossaryTempData
          });
        }
        return glossaryData;
      }
    },
    methods: {
      createAnchorArray: function () {
        var vm = this;
        vm.obj2 = {};
        setTimeout(function () {
          $("div[id*='FRAME']").each(function (index) {
            var offset = 10;
            var data_attr = $(this).attr("id");
            vm.obj2[data_attr] = $(this).position().top + offset;
          });
        }, 200);
      },
      chooseGlossaryTitle: function ($event, title, index) {
        var vm = this;
        $('.lmm-menu__glossary-link').each(function () {
          $(this).removeClass('active');
        });
        var obj = this.obj;
        vm.curTitleIndex = index;
        vm.preventScrollIndexDetection = true;
        setTimeout(function () {
          $(".lmm-menu__content").animate({
            scrollTop: obj[title.title]
          }, 500, function () {
            vm.curTitleIndex = index;
            vm.preventScrollIndexDetection = false;
          });
        }, 100);
        return false;
      },
      juxtaposeContents: function () {
        setTimeout(function () {
          $('li.slide-item').each(function (index, el) {
            var dataSlideId = $(el).attr('data-slide-id');
            var item = $('.lmm-menu-content-item[data-slide-id="' +
                         dataSlideId + '"]');
            if ($(el).hasClass('forbidden')) {
              item.addClass('forbidden');
            }
            if ($(el).hasClass('visited')) {
              item.addClass('visited');
            }
            if ($(el).hasClass('active')) {
              item.find('.lmm-menu__nav-sublink').removeClass('disable');
            }
          })
        }, 0)
      },
      openMenu: function () {
        var vm = this;
        $(this.$el).closest("#TXT_2").css('z-index', 1005);
        this.currentMenuItemIndex = $('#contents_old .slide-item.current').index();
        this.contents();
        this.currentMenuItemSubIndex = this.getSubMenuIndex();
        this.juxtaposeContents();
        $(".baron__scroller").addClass("off-scroll");
        this.$nextTick(function () {
          vm.show = true;
        });
      },
      closeMenu: function () {
        this.contents();
        this.show = false;
        $('.baron__scroller').removeClass('off-scroll');
        $(this.$el).closest("#TXT_2").css('z-index', '');
      },
      contents: function () {
        $('.lmm-menu__content .lmm-menu__nav-item.active').removeClass('active');
        current = $('#contents_old .slide-item.current').index();
        $('.lmm-menu__content .lmm-menu__nav-item').eq(current).addClass('active');
        $('#contents_old_default .unvisited').removeClass('allowed').addClass('forbidden').off();
      },
      getSubMenuIndex: function () {
        var anchorArray = $("[anchor]");
        var offsetIndex;
        var minOffset = -Infinity;
        for (var i = 0; i < anchorArray.length; i++) {
          var offset = $(anchorArray[i]).offset().top;
          if (offset <= 200 && Math.max(minOffset, offset) !== minOffset) {
            offsetIndex = i;
          }
        }
        return offsetIndex;
      },
      changeTab: function ($event, index) {
        this.tabIndex = index;
        var obj = this.obj;
        $(".lmm-menu__glossary-link").removeClass("active");
        $(".lmm-menu__glossary-link").eq(0).addClass("active");
        $(".lmm-menu__content").animate({
          scrollTop: 0
        }, 0);
        $(this.$refs.scrollContent).scrollTop(0);
        this.$nextTick(function () {
          if (index === 2 && Object.keys(obj).length === 0) {
            var offset_first = 0;
            $(".lmm-menu__glossary-title").each(function (index) {
              if (index === 0) {
                offset_first = $(this).offset().top;
              }
              var data_attr = $(this).attr("data-title-id");
              obj[data_attr] = $(this).offset().top - offset_first + 0.1 * $(
                this).parent().prevAll().find(
                ".lmm-menu__glossary-text").length;
            });
          }
        });
      },
      handleMaterial: function (material) {
        window.open(material.link, 'newwindow', 'width=' + screen.availWidth + ',height=' +
                    screen.availHeight);
      },
      chooseTheme: function ($event, menuItem, index, subMenuItem, subIndex) {
        if (!!menuItem.disabled) {
          return;
        }
        this.closeMenu();
        if (String(menuItem.id) != '') {
          var jxFrame = CL.axSlides.find("frame").closest("slide[id='" + menuItem.id + "']").find(
            'frame');
          var frameID = jxFrame.attr("id");
          var jxSlide = jxFrame.parents('slide:first');
          sSlideId = jxSlide.attr('id');
          if (jxSlide.attr('id') !== CLZ.sCurrentSlideId) {
            CL.Open.Slide({
              slideid: sSlideId
            });
            EventBus.$emit('create-anchor-array');
            if (!CLF[frameID].bIsFirst) {
              CLF[frameID].Start();
              EventBus.$emit('create-anchor-array');
            }
          } else if (!subMenuItem) {
$('.baron__scroller:visible').animate({
scrollTop: 0
}, 1500);
} else {
EventBus.$emit('create-anchor-array');
}
        }
        var vm = this;
        setTimeout(function () {
          if (!!subMenuItem) {
            if (!!subMenuItem.id) {
              var obj2 = vm.obj2;
              setTimeout(function () {
                $(".lmm-overflow-y-auto").animate({
                  scrollTop: $(".lmm-overflow-y-auto")
                  .scrollTop() + obj2[subMenuItem.id]
                }, 1500);
              }, 100);
            }
          }
        }, 100)
      }
    }
  });
</script>
